{
    "name": "demo/stripe-cli-demo",
    "description": "WordPress plugin for Stripe CLI webhook testing demo",
    "type": "wordpress-plugin",
    "require": {
        "stripe/stripe-php": "^13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
